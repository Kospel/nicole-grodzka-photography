{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "routing/*": ["root/routing/*"],
      "pages/*": ["root/pages/*"],
      "common/*": ["root/common/*"],
      "assets/*": ["root/assets/*"],
      "utils/*": ["root/utils/*"]
    }
  }
}
